{
  "testFiles": "**/*.feature",
  "viewportWidth": 1024,
  "viewportHeight": 768,
  "videoCompression": 18,
  "experimentalSessionAndOrigin": true,
  "screenshotsFolder": "cypress/reports/assets",
  "video": false,
  "screenshotOnRunFailure": true,
  "reporter": "mochawesome",
  "reporterOptions": {
    "reportDir": "cypress/reports",
    "reportFilename": "[status]-[name]-report",
    "reportTitle": "E2E Test Report",
    "quite": true,
    "overwrite": false,
    "html": false,
    "json": true
  }

}
